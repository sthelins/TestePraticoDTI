package canis;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		CaninoFeliz C1 = new CaninoFeliz();
		C1.setDistanciaKm(2);
		
		System.out.println("Caes pequenos: ");
		C1.setCaesPequenos(entrada.nextInt());
		
		System.out.println("Caes Grandes: ");
		C1.setCaesGrandes(entrada.nextInt());
		C1.setDiaDaSemana("quarta");
		
		System.out.println(C1.getCaesPequenos());
		
		C1.Precificacao(C1.getCaesPequenos(), C1.getCaesGrandes());

	}

}
