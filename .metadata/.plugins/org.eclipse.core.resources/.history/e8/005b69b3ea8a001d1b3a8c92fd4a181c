package canis;

public class CaninoFeliz extends Atributos{
	
	
	public double Precificacao(int CaesPequenos, int CaesGrandes, int DiaDaSemana, double Preco) {
		if(DiaDaSemana >= 1 && DiaDaSemana <= 5) {
			Preco = (CaesPequenos * 20 ) + (CaesGrandes * 40);
		}else if(DiaDaSemana <=0 && DiaDaSemana >=6) {
			Preco = (CaesPequenos * 24) + (CaesGrandes * 48);
		}
		return Preco;
	}
	
	
}
